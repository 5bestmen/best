<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.corsair.entity.ApplicationInformation" table="sys_application" catalog="DMS" dynamic-insert="true" dynamic-update="true">
        <id name="key" type="java.lang.String">
            <column name="KEY" length="64"/>
        </id>
        <property name="value" type="java.lang.String">
            <column name="VALUE" length="64" not-null="true">
            </column>
        </property>
    </class>
</hibernate-mapping>
