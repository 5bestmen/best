<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.corsair.entity.InverterPowerPoint" catalog="DMS" dynamic-insert="true" dynamic-update="true">
        <id name="ID">
            <generator class="uuid"/>
        </id>
        <property name="point" type="java.lang.Integer" not-null="true"/>
    </class>
</hibernate-mapping>

