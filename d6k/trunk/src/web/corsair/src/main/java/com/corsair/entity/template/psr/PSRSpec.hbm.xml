<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.corsair.entity.template.psr.PSRSpec" catalog="DMS" dynamic-insert="true" dynamic-update="true">
        <id name="id">
            <generator class="uuid"/>
        </id>
        <property name="index" column="specIndex" type="java.lang.Integer" not-null="true" />
        <property name="key" type="java.lang.String" not-null="true" />
        <property name="value" type="java.lang.String" not-null="true" />
    </class>
</hibernate-mapping>
