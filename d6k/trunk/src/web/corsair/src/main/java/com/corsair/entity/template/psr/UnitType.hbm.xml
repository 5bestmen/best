<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.corsair.entity.template.psr.UnitType" catalog="DMS" dynamic-insert="true" dynamic-update="true">
        <id name="id">
            <generator class="uuid"/>
        </id>
        <property name="comments" type="java.lang.String"/>
        <property name="dataUnit" type="java.lang.String" not-null="true" unique="true"/>
        <property name="chartUnit" type="java.lang.String"/>
    </class>
</hibernate-mapping>
